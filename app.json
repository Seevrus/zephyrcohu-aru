{
  "expo": {
    "name": "Zephyr Boreal",
    "slug": "zephyr-boreal",
    "version": "0.8.2",
    "orientation": "portrait",
    "icon": "./react_native/assets/expo-app-icons/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "icon": "./react_native/assets/expo-app-icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "package": "com.zephyr.boreal",
      "versionCode": 21,
      "adaptiveIcon": {
        "foregroundImage": "./react_native/assets/expo-app-icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "45d5460b-c962-4e8a-ba8e-55d35d513f2d"
      }
    },
    "owner": "seevrus",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Kérem engedélyezze a Zephyr Boreal számára a kamera használatát.",
          "microphonePermission": "Kérem engedélyezze a Zephyr Boreal számára a mikrofon használatát.",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}
